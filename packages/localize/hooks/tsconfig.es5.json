{
  "extends": "../../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "../../../dist/out-tsc",
		"rootDir": ".",
    "module": "commonjs",
    "target": "es2015",
    "moduleResolution": "node",
    "declaration": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noEmitHelpers": false,
    "noEmitOnError": true,
    "removeComments": true
  },
	"include": ["**/*.ts"]
}
